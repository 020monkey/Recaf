<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
		"-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
		"https://checkstyle.org/dtds/suppressions_1_0.dtd">
<suppressions>
	<!-- Allow switch statement on instruction types -->
	<suppress checks="CyclomaticComplexity"
			  files="Disassembler.java"
			  lines="0-9999"/>
	<!-- Allow many methods in cases where it's unavoidable/makes sense to do so -->
	<suppress checks="ClassFanOutComplexity"
			  files="Disassembler.java"
			  lines="0-9999"/>
	<!-- Similar reasons for the disassembler -->
	<suppress checks="CyclomaticComplexity"
			  files="VMWrap.java"
			  lines="0-9999"/>
	<suppress checks="ClassFanOutComplexity"
			  files="VMWrap.java"
			  lines="0-9999"/>
</suppressions>
